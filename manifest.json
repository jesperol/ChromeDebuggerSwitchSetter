{
  "name": "Force Emulation Starter",
  "description": "Lets you turn on the Force Dark Mode and Emulate Focus debugger switches from the toolbar.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting", "debugger"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/images/darken_16.png",
    "32": "/images/darken_32.png",
    "48": "/images/darken_48.png",
    "128": "/images/darken_128.png"
  }
}
